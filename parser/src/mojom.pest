line_comment = { "//" ~ (!NEWLINE ~ ANY)* }
block_comment = { "/*" ~ (!"*/" ~ ANY)* ~ "*/" }
COMMENT = _{ line_comment | block_comment }

WHITESPACE = _{ " " | "\t" | NEWLINE }

char = { ASCII_ALPHA | "_" }

identifier = @{ char ~ (char | ASCII_DIGIT)* }

interface = { "interface" ~ identifier ~ "{" ~ "}" ~ ";" }

mojom_file = {
  SOI ~ (interface)* ~ EOI
}
